      "version": "1.1.0"
    },
    {
      "name": "mod_swiper_slider.swiper-bundle",
      "type": "style",
      "uri": "mod_swiper_slider/css/swiper-bundle.min.css",
      "version": "1.1.0"
    },
    {
      "name": "mod_swiper_slider.swiper-custom",
      "type": "style",
      "uri": "mod_swiper_slider/css/swiper-custom.css",
      "version": "1.1.0"
    }
